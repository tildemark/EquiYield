{
  "name": "equiyield-monorepo",
  "private": true,
  "workspaces": [
    "apps/server",
    "apps/web"
  ],
  "scripts": {
    "dev": "concurrently -n server,web -c blue,green \"npm:dev --workspace=apps/server\" \"npm:dev --workspace=apps/web\"",
    "build": "npm run build --workspace=apps/server && npm run build --workspace=apps/web",
    "prisma:generate": "npm run prisma:generate --workspace=apps/server"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "concurrently": "^9.0.1"
  }
}
